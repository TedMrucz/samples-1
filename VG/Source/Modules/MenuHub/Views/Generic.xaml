  <ctrl:ImageConverter x:Key="ImageConverter" />
  <ctrl:ColorBrushConverter x:Key="BrushConverter" />
  <local:TileItemDataTemplateSelector x:Key="TileItemDataTemplateSelector" />

  <Style x:Key="TileSimpleBtnStyle" TargetType="{x:Type Button}">
    <Setter Property="BorderThickness" Value="0"/>
    <Setter Property="Focusable" Value="False"/>
    <Setter Property="IsTabStop" Value="False"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Button}">
          <ContentPresenter />
          <ControlTemplate.Triggers>
            <Trigger Property="IsPressed" Value="true">
              <Setter Property="Opacity" Value="0.6" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

  <DataTemplate x:Key="TileBtnDataTemplate" >
    <Button Style="{StaticResource TileSimpleBtnStyle}" Command="{Binding Path=VM.TileCommandEvent}" CommandParameter="{Binding Path=Index}">
      <Border BorderThickness="1" Margin="4,4,4,4" BorderBrush="{DynamicResource ButtonBorder}" Background="{DynamicResource ButtonBackground}">
        <StackPanel Width="190">
          <StackPanel Orientation="Horizontal" Height="86">
            <Image Source="{Binding Path=Image, Converter={StaticResource ImageConverter}}" Width="84" Height="84"/>
            <TextBlock Text="{Binding Path=TextValue}" TextWrapping="Wrap" Width="150" FontSize="18" Margin="-8,0,0,0" Foreground="{Binding Path=BkgBrush, Converter={StaticResource BrushConverter}}" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
          </StackPanel>
          <StackPanel Margin="2,-16,2,0">
            <TextBlock Text="{Binding Path=Label}" TextWrapping="Wrap" FontSize="14" Width="150" HorizontalAlignment="Center" Foreground="{DynamicResource ButtonText}"/>
          </StackPanel>
        </StackPanel>
      </Border>
    </Button>
  </DataTemplate>

  <DataTemplate x:Key="TileBtnRoundDataTemplate" >
    <Button Style="{StaticResource TileSimpleBtnStyle}" Command="{Binding Path=VM.TileCommandEvent}" CommandParameter="{Binding Path=Index}">
      <Grid>
        <Ellipse Stroke="Silver" StrokeThickness="1" Margin="4,4,4,4" Fill="White" />
        <StackPanel Width="200">
          <StackPanel Orientation="Horizontal" Height="86">
            <Image Source="{Binding Path=Image, Converter={StaticResource ImageConverter}}" Width="84" Height="84"/>
            <TextBlock Text="{Binding Path=TextValue}" TextWrapping="Wrap" Width="150" FontSize="18" Margin="-8,0,0,0" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{Binding Path=BkgBrush, Converter={StaticResource BrushConverter}}" />
          </StackPanel>
          <StackPanel Margin="2,-16,2,0">
            <TextBlock Text="{Binding Path=Label}" TextWrapping="Wrap" FontSize="14" Height="44" HorizontalAlignment="Center"/>
          </StackPanel>
        </StackPanel>
      </Grid>
    </Button>
  </DataTemplate>

  <DataTemplate x:Key="TileBtnSpinDataTemplate" >
    <Button Style="{StaticResource TileSimpleBtnStyle}" Command="{Binding Path=VM.TileCommandEvent}" CommandParameter="{Binding Path=Index}">
      <Border BorderBrush="Silver" BorderThickness="1" Margin="4,4,4,4" Background="PaleGreen">
        <mui:Ticker Style="{StaticResource sd:UpDownBase}" Interval="3" Width="190">
          <Grid>
            <Grid Height="90">
              <Image Source="{Binding Path=Image, Converter={StaticResource ImageConverter}}" Width="84" Height="84" HorizontalAlignment="Left" VerticalAlignment="Top"/>
              <TextBlock Text="{Binding Path=TextValue}" TextWrapping="Wrap" FontWeight="Bold" Width="108" Height="48" FontSize="18" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="{Binding Path=BkgBrush, Converter={StaticResource BrushConverter}}" />
            </Grid>
            <TextBlock Text="{Binding Path=Label}" TextWrapping="Wrap" FontSize="14" Width="150" Height="32" Margin="0,8,0,0" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
          </Grid>
          <TextBlock Text="{Binding Path=TextValue}" TextWrapping="Wrap" Width="150" FontSize="18" Foreground="{Binding Path=BkgBrush, Converter={StaticResource BrushConverter}}" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </mui:Ticker>
      </Border>
    </Button>
  </DataTemplate>

